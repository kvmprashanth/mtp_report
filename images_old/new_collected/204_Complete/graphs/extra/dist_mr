set term png size 1200, 800
set output "../Dist_MR.png"
set title 'Distributive plot between Time - Memory Reclaimed'
set xlabel 'Time (s)'
set ylabel 'Memory Reclaimed (MB)'
plot '../dist_mr.data' using 1:(($2)/1048576) with linespoints t 'M1', \
	 '../dist_mr.data' using 1:(($3)/1048576) with linespoints t 'M2'
